<snippet name="Room" styles="./room/roomStyle.css">

<div id="contents">
    <div id="bg"></div>
    <div id="passages"><div class="passage">
        <div id="screenBg"></div>

        <div id="screenContents" class="absoluteAlign">
            <div id="tophalf" class="absoluteAlign">
                <div id="roomName" class="blackBg2"></div>
            </div>
            <div id="bottomhalf" class="absoluteAlign blackBg">
                <div id="bottomContent" class="blackBg2">
                </div>
                <div id="menu" class="blackBg2">
                    <button type="button" id="inventory">Inventory</button>
                    <button type="button" id="map">Map</button>
                    <button type="button" id="party">Party</button>
                </div>
            </div>
        </div>
    </div></div>
</div>

<%
$(function () {
    const ref = window.vapormall;
    ref.GameState.currentFloor.renderRoom();
});
%>

</snippet>